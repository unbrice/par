<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>PhoneWaker</servlet-name>
		<servlet-class>net.vleu.par.gateway.PhoneWakerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PhoneWaker</servlet-name>
		<url-pattern>/_ah/queue/phoneWakeupsQueue</url-pattern>
	</servlet-mapping>

	<security-constraint>
		<web-resource-collection>
			<web-resource-name>TasksQueues</web-resource-name>
			<url-pattern>/_ah/queue/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>admin</role-name>
		</auth-constraint>
	</security-constraint>
</web-app>
